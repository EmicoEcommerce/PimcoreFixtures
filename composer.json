{
  "name": "youwe/pimcore-fixtures",
  "description": "Load yml fixtures in pimcore",
  "type": "pimcore-bundle",
  "require": {
    "pimcore/pimcore": "^6.1",
    "nelmio/alice": "2.2.*",
    "fzaninotto/faker": "1.6.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Manea Eugen"
    },    
    {
      "name": "Joris Ros",
      "email": "j.ros@youwe.nl"
    },
    {
      "name": "Youwe"
    }
  ],
  "autoload": {
    "psr-4": {
      "FixtureBundle\\": "/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "FixtureBundle\\FixtureBundle"
      ]
    }
  }
}
